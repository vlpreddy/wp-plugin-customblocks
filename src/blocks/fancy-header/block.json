{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name":"roudram-create-blocks/fancy-header",
    "title": "Fancy Header",
    "category":"text",
    "icon":"star-filled",
    "description": "Adds a header with a hover effect",
    "keywords": [
        "header","underline","hover"
    ],
    "version": "1",
    "textdomain": "roudram-create-blocks",
    "editorScript":"file:./index.js",
    "attributes": {
        "content" : {
            "type":"string",
            "source":"html",
            "selector": "h2"
        },
        "underline_color":{
            "type":"string",
            "default": "#f87171"
        }
    },
    "style":"file:./index.css"
}